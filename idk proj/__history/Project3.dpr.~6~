program Project3;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var
  a: integer;
  endp: string;

begin
  try
    { TODO -oUser -cConsole Main : Insert code here }
    readln(a);
    writeln(a + a);
  except
    on E: Exception do
      Writeln(E.ClassName, ': ', E.Message);
  end;
  writeln("для завершения программы нажмите Enter . . . ");
  readln(endp);
end.
